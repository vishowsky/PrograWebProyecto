@model PrograWebProyecto.Models.Cargo

@{
    ViewData["Title"] = "Edit";
}

<h1>Editar Cargo</h1>

<h4>Cargo</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Id" />
            <div class="form-group">
                <label asp-for="NombreCargo" class="control-label"></label>
                <input asp-for="NombreCargo" class="form-control" />
                <span asp-validation-for="NombreCargo" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Guardar" class="btn btn-danger" /> |
                <a class="btn btn-danger" asp-action="Index">Volver Atras</a>
            </div>
        </form>
    </div>
</div>



@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
